{
  "name": "gofa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "esw .",
    "lint:watch": "yarn lint --watch",
    "logs-android": "react-native logs-android",
    "logs-ios": "react-native logs-ios",
    "test": "jest",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"**/*.js\""
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-native": "0.56.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "^5",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "husky": "^0.14.3",
    "jest": "23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react-test-renderer": "16.4.1"
  }
}
